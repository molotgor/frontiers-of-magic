  "flags": {
    "frontiers-of-magic": {
      "pf2e-homebrew": {
        "spellTraits": {
          "consolation": {
            "label": "Consolation",
            "description": "A consolation cantrip is a spell you can cast immediately after another spell ends abruptly or produces no effect. Because these spells are situationally relevant and don’t offer the same impact as other cantrips, whenever you would prepare one cantrip for the day or add one cantrip to your spell repertoire, you can prepare or add two consolation cantrips, instead"
          },
          "cue": {
            "label": "Cue",
            "description": "This is a Cue Manipulation spell"
          },
          "stare": {
            "label": "Stare",
            "description": "This is a Stare Manipulation spell"
          },
          "treatment": {
            "label": "Treatment",
            "description": "This is a Treatment Manipulation spell"
          },
          "mesmerist": {
            "label": "Mesmerist",
            "description": "This indicates abilities from the mesmerist class."
          },
          "secret": {
            "label": "Secret",
            "description": "The GM rolls the Check for this ability in secret."
          },
          "shifter":{
            "label": "Shifter",
            "description": "This indicates abilities from the shifter class."
          }
        },
        "featTraits": {
          "familiar": {
            "label": "Familiar",
            "description": "A creature with this trait is a member of the familiar ancestry. An ability with this trait can be used or selected only by familiars. An item with this trait is created and used by familiars."
          },
          "imbued": "Imbued",
          "mesmerist": {
            "label": "Mesmerist",
            "description": "This indicates abilities from the mesmerist class."
          },
          "consult": {
            "label": "Consult",
            "description": "You can only use this action if consultant has a reaction available and if they are willing to use it as described in the consult action."
          },
          "expertise": {
            "label": "Expertise",
            "description": "Depending on the outcome, this action can designate a creature as the target of your expertise. You can have only one expertise target at a time, and designating a new expertise target causes the previous target to lose that designation."
          },
          "shifter": {
            "label": "Shifter",
            "description": "This indicates abilities from the shifter class."
          }
        },
        "weaponTraits": {
          "spiritcrafted": {
            "label": "Spiritcrafted",
            "description": "Items with this trait indicate that it was made with Spirit Crafting feat."
          },
          "shifter-form-attack": {
            "label": "Shifter Form Attack",
            "description": "You got this strike from shifter form."
          },
          "shift-attack": {
            "label": "Shift Attack",
            "description": "You got this strike from shift."
          }
        },
        "equipmentTraits": {
          "spiritcrafted": {
            "label": "Spiritcrafted",
            "description": "Items with this trait indicate that it was made with Spirit Crafting feat."
          }
        }
      }
    }
  },
