        "spellsTraits": {
          "consolation": {
            "label": "Consolation",
            "description": "A consolation cantrip is a spell you can cast immediately after another spell ends abruptly or produces no effect. Because these spells are situationally relevant and donâ€™t offer the same impact as other cantrips, whenever you would prepare one cantrip for the day or add one cantrip to your spell repertoire, you can prepare or add two consolation cantrips, instead"
          },
          "cue": {
            "label": "Cue",
            "description": "This is a Cue Manipulation spell"
          },
          "stare": {
            "label": "Stare",
            "description": "This is a Stare Manipulation spell"
          },
          "treatment": {
            "label": "Treatment",
            "description": "This is a Treatment Manipulation spell"
          },
          "mesmerist": {
            "label": "Mesmerist",
            "description": "This indicates abilities from the mesmerist class."
          },
          "secret": {
            "label": "Secret",
            "description": "The GM rolls the Check for this ability in secret."
          }
          
        },
        "featTraits": {
          "familiar": {
            "label": "Familiar",
            "description": "A creature with this trait is a member of the familiar ancestry. An ability with this trait can be used or selected only by familiars. An item with this trait is created and used by familiars."
          },
          "imbued": "Imbued",
          "mesmerist": {
            "label": "Mesmerist",
            "description": "This indicates abilities from the mesmerist class."
          }
        }
      }
    }
